<form class="form-horizontal">
    <div class="form-group">
        <label for="assessment-selection" class="col-md-5  control-label">{{t "results:assessment_selection.select_label"}}</label>
        <div class="col-md-7">
            <select id="assessment-selection" class="form-control">
                <option value="" selected disabled>{{t "results:assessment_selection.select_tooltip"}}</option>
                {{#each list}}
                    <option data-model-id="{{_id}}" {{#unless this.permissions.results}}disabled{{/unless}}>{{this.title}}{{#unless this.permissions.results}}{{t "results:assessment_selection.no_results"}}{{/unless}}</option>
                {{/each}}
            </select>
        </div>
    </div>
</form>
