{
  "src_folders": [
    "tests/e2e/subjects"
  ],
  "output_folder": "reports",
  "custom_commands_path": "tests/e2e/commands",
  "custom_assertions_path": "",
  "page_objects_path": [
    "tests/e2e/pages"
  ],
  "globals_path": "",
  "selenium": {
    "start_process": true,
    "server_path": "bin/selenium-server-standalone-2.53.0.jar",
    "log_path": "",
    "host": "127.0.0.1",
    "port": 4444,
    "cli_args": {
      "webdriver.chrome.driver": "",
      "webdriver.ie.driver": ""
    }
  },
  "test_settings": {
    "default": {
      "launch_url": "http://localhost:9000",
      "selenium_port": 4444,
      "selenium_host": "localhost",
      "silent": true,
      "screenshots": {
        "enabled": true,
        "path": "screenshots"
      },
      "globals": {
        "timeout": 5000
      },
      "desiredCapabilities": {
        "browserName": "firefox",
        "javascriptEnabled": true,
        "acceptSslCerts": true
      }
    },
    "saucelabs-local": {
      "selenium_host": "ondemand.saucelabs.com",
      "selenium_port": 80,
      "username": "${SAUCE_USERNAME}",
      "access_key": "${SAUCE_ACCESS_KEY}",
      "use_ssl": false,
      "silent": true,
      "output": true,
      "screenshots": {
        "enabled": false,
        "on_failure": true,
        "path": "screenshots"
      },
      "desiredCapabilities": {
        "name": "test-firefox",
        "browserName": "firefox"
      },
      "selenium": {
        "start_process": false
      }
    },
    "chrome-win32": {
      "desiredCapabilities": {
        "browserName": "chrome",
        "javascriptEnabled": true,
        "acceptSslCerts": true
      },
      "cli_args": {
        "webdriver.chrome.driver": "bin/chrome-webdriver-2.22/win32/chromedriver.exe"
      }
    },
    "chrome-mac32": {
      "desiredCapabilities": {
        "browserName": "chrome",
        "javascriptEnabled": true,
        "acceptSslCerts": true
      },
      "cli_args": {
        "webdriver.chrome.driver": "bin/chrome-webdriver-2.22/mac32/chromedriver"
      }
    }
  }
}
